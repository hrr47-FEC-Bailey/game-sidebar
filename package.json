{
  "name": "game-sidebar",
  "version": "1.0.0",
  "description": "Game Sidebar component",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-dev": "nodemon index.js",
    "build-dev": "webpack -d --watch",
    "build": "webpack -d",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrr47-FEC-Bailey/purchase-options.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hrr47-FEC-Bailey/purchase-options/issues"
  },
  "homepage": "https://github.com/hrr47-FEC-Bailey/purchase-options#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mysql": "^2.18.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.4.0",
    "jest-css-modules": "^2.1.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFiles": [
      "<rootDir>/enzyme.config.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "expectedResults.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true
  }
}
